  "homepage": "http://meanjs.org/",
  "license": "MIT",
  "dependencies": {
    "angular": "~1.3",
    "angular-resource": "~1.3",
    "angular-animate": "~1.3",
    "angular-messages": "~1.3",
    "angular-mocks": "~1.3",
    "angular-ui-notification": "~0.2.0",
    "angular-ui-router": "~0.2.10",
